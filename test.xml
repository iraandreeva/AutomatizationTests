<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestSuite">
    <test name="AutoQA">
        <parameter name="num1" value="2"/>
        <parameter name="num2" value="2"/>
        <classes>
            <class name="TestClass">
            <methods>
                <include name = "testEquals"/>
                <include name = "testNotEquals"/>
                <include name = "testTrue"/>
                <include name = "testFalse"/>
                <include name = "testFail"/>
                <include name = "testParametrized"/>
                <include name = "testSoftAssert"/>
            </methods>
            </class>
        </classes>
    </test>
</suite>

